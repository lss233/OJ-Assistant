{
	"name": "oj-assistant",
	"displayName": "OJ Assistant",
	"description": "Solving OJ problems in your VSCode.",
	"publisher": "SparkleL",
	"preview": true,
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Debuggers",
		"Other"
	],
	"activationEvents": [
		"workspaceContains:.ojassistconfig.json"
	],
	"repository": {
		"url": "https://github.com/lss233/oj-assistant"
	},
	"main": "./dist/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"type": "webview",
					"id": "ojassistant.problemInfoView",
					"name": "Problem Info"
				}
			]
		},
		"commands": [
			{
				"command": "ojassist.submit",
				"title": "OJ-Assist: Submit this code"
			},
			{
				"command": "ojassist.data",
				"title": "OJ-Assist: Open data file"
			},
			{
				"command": "ojassist.reload",
				"title": "OJ-Assist: Reload config"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack --config ./build/node-extension.webpack.config.js",
		"watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
		"package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.14.0",
		"@typescript-eslint/parser": "^4.14.0",
		"eslint": "^7.18.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"ts-loader": "^8.0.11",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1",
		"webpack": "^5.17.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"@3846masa/axios-cookiejar-support": "^1.0.1",
		"@types/cheerio": "^0.22.23",
		"@types/qs": "^6.9.5",
		"@types/tough-cookie": "^4.0.0",
		"axios": "^0.21.1",
		"cheerio": "^1.0.0-rc.5",
		"qs": "^6.9.6",
		"tough-cookie": "^4.0.0"
	}
}
